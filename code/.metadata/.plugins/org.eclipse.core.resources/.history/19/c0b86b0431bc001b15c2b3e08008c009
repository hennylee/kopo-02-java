package kr.ac.kopo.day16;

public class UserInfo {
	private String name;
	private int age;
	private String addr;
	// 기본 생성자 : alt + shift + s + c
	public UserInfo() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	// 매개변수 생성자
	public UserInfo(String name, int age, String addr) {
		super();
		this.name = name;
		this.age = age;
		this.addr = addr;
	}
	
	
	// getter/setter
}
