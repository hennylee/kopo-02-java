/*

-- 테이블 확인
SELECT * FROM EMPLOYEES;
SELECT * FROM JOBS;

-- JOIN 전 쿼리
SELECT JOB_ID, SUM(SALARY)/COUNT(EMPLOYEE_ID) 
FROM EMPLOYEES 
WHERE SALARY > 2000 
GROUP BY JOB_ID 
ORDER BY JOB_ID;

-- JOIN 후 쿼리
SELECT j.JOB_TITLE, SUM(e.SALARY)/COUNT(e.EMPLOYEE_ID)
FROM EMPLOYEES e, JOBS j
WHERE e.JOB_ID = j.JOB_ID AND e.SALARY > 10000
GROUP BY j.JOB_TITLE;

 */


