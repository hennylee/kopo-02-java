/**
 * 게시판들을 관리하는 것이 목적이다.
 * @author HP
 *
 */
import kr.ac.kopo.vo.BoardVO;

public class BoardDAO {
	private List<BoardVO> list;
	
	public BoardDAO() {
		list = new ArrayList<>();
	}
	
	/**
	 * 전체 게시글 조회
	 */
	public List<BoardVO> 전체게시글조회() throws Exception {
		return list;
	}
	
	/**
	 * 글번호로 게시글 조회
	 */
	
}
